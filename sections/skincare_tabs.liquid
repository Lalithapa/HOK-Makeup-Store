      // Add click event to each tab
      tabs.forEach(tab => {
        tab.addEventListener('click', () => {
          // Remove active class from all tabs
          tabs.forEach(item => item.classList.remove('active'));
          // Add active class to the clicked tab
          tab.classList.add('active');
