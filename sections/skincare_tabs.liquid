{% style %}
#about:checked ~ .text-skincare-content .skincare_product_type,
#code:checked ~ .text-skincare-content .skincare_product_type,
#skincare_concern:checked ~ .text-skincare-content .skincare_product_type,
#skincare_ingredients:checked ~ .text-skincare-content .skincare_product_type,
.skin-tab-panel,
.skincare-content .text,
.skincare-content input {
    display: none;
}
.skincare-content {
    display: flex;
    justify-content: space-evenly;
    align-items: center;
    padding: 10rem 0;
    gap: 10px;
}
.skin-sidebar,
.skincare-content .list {
    height: 56rem;
    overflow: hidden;
}
.skincare-content .list {
    display: flex;
    flex-direction: column;
    align-items: stretch;
    position: relative;
    width: 12%;
    gap: 20px;
}
.skincare-content .list label {
    cursor: pointer;
    font-size: 20px;
    transition: 0.5s;
    z-index: 1;
    flex: 1;
    aspect-ratio: 1;
}
#skincare_concern:checked ~ .list label.skincare_concern,
#skincare_ingredients:checked ~ .list label.skincare_ingredients,
#skincare_product_type:checked ~ .list label.skincare_product_type {
    color: #22272c;
    background: #e3dfd0;
    box-shadow: 0 4px 10px 0 #d8d5ca;
}
.skincare-content .text-skincare-content {
    width: 85%;
    height: 100%;
    color: rgba(0, 0, 0, 0.7);
}
.w-10 {
    width: 10%;
}
.skincare-content .text .title {
    font-size: 25px;
    margin-bottom: 10px;
    font-weight: 500;
}
#skincare-container .text .para {
    margin-top: 6px;
    font-size: clamp(10px, 2vw, 14px);
    text-align: center;
}
.skincare_banner_header {
    color: #000;
    font-family: var(--header-family);
    font-size: 7rem;
    text-transform: uppercase;
}
.skincare_banner_content {
    color: #000;
    font-family: var(--family-light);
    font-size: 2rem;
}
.skincare-content .text-skincare-content .skincare_product_type {
    display: block;
}
#skincare_concern:checked ~ .text-skincare-content .skincare_concern,
#skincare_ingredients:checked ~ .text-skincare-content .skincare_ingredients,
#skincare_product_type:checked ~ .text-skincare-content .skincare_product_type {
    display: flex;
    justify-content: space-between;
    overflow-x: hidden;
}
.main-tab-title {
    height: 100%;
    flex-direction: column;
    text-align: center;
    border-radius: 6px;
    background: #f4f2eb;
    box-shadow: 0 4px 10px 0 #d8d5ca;
    max-width: calc(52rem / 3);
}
.skin-sidebar {
    position: relative;
    flex: 0 0 10%;
    display: flex;
    flex-direction: column;
    gap: 10px;
    max-width: 200px;
    justify-self: center;
}
.skin-tab-item {
    cursor: pointer;
    border-radius: 5px;
    transition: background-color 0.3s;
    color: #000;
    text-align: center;
    font-family: var(--family-regular);
}
.skin-tab-item img {
    object-fit: contain;
    max-width: 130px;
    max-height: 130px;
    border-radius: 15px;
    border: 3px solid #f6f6f6;
    background: #fff;
    box-shadow: 0 2px 4px 0 rgba(224, 213, 213, 0.25);
}
.skin-tab-item.active {
    color: #000;
    font-family: var(--family-bold);
}
.skin-tab-item.active img {
    filter: brightness(50%);
    border: 3px solid #ffffff;
}
.tab-skincare-content {
    flex: 3;
    margin: 0 0 16px 20px;
    flex: 0 0 85%;
    overflow-x: auto;
    padding-bottom: clamp(16px, 2vw, 32px);
}
.skin-tab-panel .b-product_promo_tile {
    max-width: 320px;
}
.skin-tab-panel.active {
    display: flex;
    gap: 20px;
}
.tab-skincare-content::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 2px rgba(0, 0, 0, 0.2);
    background-color: #f5f5f5;
    border-radius: 4px;
}
.tab-skincare-content::-webkit-scrollbar {
    height: 3px;
    background-color: #f5f5f5;
}
.tab-skincare-content::-webkit-scrollbar-thumb {
    background-color: #353535;
    border-radius: 4px;
}
.product-tab-card {
    flex: 1;
    min-width: 320px;
    max-width: 320px;
}
.skincare-arrow_handler {
    display: none !important;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    z-index: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    height: 560px;
}
@media screen and (max-width: 990px) {
    .sm-margin {
        margin: 26px 0 18px;
    }
    .skincare-content {
        width: calc(100% - 16px);
        margin: -15px auto 0;
        padding: 15px 15px 0;
        gap: 16px;
        border-top-left-radius: 15px;
        border-top-right-radius: 15px;
        background: #fff;
        box-shadow: 0 4px 20px 0 rgba(52, 66, 71, 0.1);
    }
    .z--1 {
        z-index: -1;
    }
    .skincare_banner_header {
        font-size: 24px;
    }
    .skincare_banner_content {
        font-size: 14px;
    }
    .square-banner-media {
        border-radius: 8px;
    }
    .skin-tab-item {
        display: flex;
        flex-direction: column !important;
    }
    .skin-tab-item img {
        object-fit: contain;
        max-width: 85px;
        max-height: 85px;
    }
    .skincare-content .list {
        flex-direction: row;
        flex-wrap: nowrap;
        width: 100%;
        height: 26px;
        gap: 5px;
        border-bottom: 0.5px solid #cdcdcd;
    }
    .skincare-arrow_handler {
        display: none;
    }
    .main-tab-title {
        background: #fff;
        box-shadow: unset;
    }
    #skincare_concern:checked ~ .list label.skincare_concern,
    #skincare_ingredients:checked ~ .list label.skincare_ingredients,
    #skincare_product_type:checked ~ .list label.skincare_product_type {
        border-radius: 6px 6px 0 0;
        background: #efeeea;
        font-family: var(--family-bold);
        box-shadow: unset;
    }
    .skincare-content .list label {
        cursor: pointer;
        font-size: 13px;
    }
    .skincare-content,
    .skincare_concern,
    .skincare_ingredients,
    .skincare_product_type {
        flex-direction: column;
    }
    .skin-sidebar {
        max-width: unset;
        height: 100%;
    }
    .skin-tab-panel .b-product_promo_tile {
        max-width: 260px;
    }
    .product-tab-card {
        min-width: 220px;
        max-width: 220px;
    }
    .skin-sidebar,
    .tab-skincare-content {
        flex: 0 0 100%;
        margin-left: 0;
    }
    .skincare-content .text-skincare-content {
        width: 100%;
    }
}
@media screen and (min-width: 990px) {
    #skincare-container {
        background: #efeeea;
    }
    .skincare-content .list {
      padding-bottom: 1rem;
    }
    .skin-tab-panel .b-product_promo_tile button.b-add_to_bag-button.button-cta {
        color: #fff;
        background: #000;
    }
    .skin-tab-panel .b-product_promo_tile button.b-add_to_bag-button.button-cta[disabled="true"]{
      background-color: #ededed;
      border: 0.5px solid #d7d7d7;
      color: #a0a0a0;
    }
    .skin-tab-panel .b-product_promo_tile button.b-add_to_bag-button.button-cta:hover {
        color: #000;
    }
}

