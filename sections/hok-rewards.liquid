<section>
  <div class="container-fluid">
    <div class="row reward-banner-container">
      <div class="col-12{% unless customer %} col-lg-6{% else %} h-100{% endunless %} reward-banner p-0">
        {% unless customer %}  
      <img
          src="{{ section.settings.rewardBanner | image_url }}"
          alt="reward-banner"
          loading="eager"
          width="100%"
          height="100%"
          class="reward-banner-img"
        >
        {% else %}
        <img
          src="{{ section.settings.rewardBanner_desktop | image_url }}"
          alt="reward-banner"
          loading="eager"
          width="100%"
          height="100%"
          class="reward-banner-img desktop_screen"
        >
        <img
          src="{{ section.settings.rewardBanner_mobile | image_url }}"
          alt="reward-banner"
          loading="eager"
          width="100%"
          height="100%"
          class="reward-banner-img mobile_screen"
        >
        {% endunless %}
      </div>
      {% unless customer %}
        <div class="col-12 col-lg-6 reward-banner-content d-flex flex-column justify-content-center gap-3 gap-md-5">
          <div class="reward-banner-text-container text-sm-center text-md-start">
            <p class="reward-banner-title">
              Join. Earn. Redeem.
              <span class="reward-banner-title-svg">
                <svg xmlns="http://www.w3.org/2000/svg" width="28" height="25" viewBox="0 0 33 30" fill="none">
                  <path d="M0 17.5C7.5 18.3333 11.6667 22.5 12.5 30C13.3333 22.5 17.5 18.3333 25 17.5C17.5 16.6667 13.3333 12.5 12.5 5C11.6667 12.5 7.5 16.6667 0 17.5Z" fill="#C2A661"/>
                  <path d="M22 5.5C25.3 5.86667 27.1333 7.7 27.5 11C27.8667 7.7 29.7 5.86667 33 5.5C29.7 5.13333 27.8667 3.3 27.5 0C27.1333 3.3 25.3 5.13333 22 5.5Z" fill="#C2A661"/>
                </svg>
              </span>
            </p>
            <p class="reward-banner-sub-title">
              Join HOK rewards for exclusive Rewards, Offers, <br>
              Discounts, Perks and more.
              <!-- Check you points <span class="reward-banner-points">here</span> -->
            </p>
            {% comment %}
              {% if customer %}
                <p class="reward-banner-title">Join. Earn. Redeem.</p>
                <p class="reward-banner-sub-title">
                  #HOK Rewards- Live Now!
                  <!-- Check you points <span class="reward-banner-points">here</span> -->
                </p>
              {% else %}
                <p class="reward-banner-title">FOMO is Real, Don't Miss Out!!</p>
                <p class="reward-banner-sub-title">
                  Join The #Hey!Fam for exclusive Rewards, Offers, Discounts, Perks and more.
                </p>
              {% endif %}
            {% endcomment %}
          </div>
          <div
            class="reward-banner-button-container d-flex align-items-center align-items-lg-start "
          >
              <!-- if user logged-in -->
              <a href="/account" class="btn-type-transparent w-100 sign_in_user">Sign In</a>
              <a href="/account" class="btn-type-transparent w-100 sign_in_user">Login</a>
          </div>
        </div>
          {% endunless %}
    </div>
  </div>
</section>
