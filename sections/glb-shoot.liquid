                    </model-viewer>
                       {% comment %} <img loading="lazy" src="https://cdn.shopify.com/s/files/1/1743/7443/files/Jessup_pdp.webp?v=1722249997" alt="product image"  width="100%" height="100%"
                           class="w-100 w-lg-75"
                           > {% endcomment %}
                    </div>
                </div>
            </div>
        </div>
<style>
  .threeD-container{
      background-image: url(https://cdn.shopify.com/s/files/1/1743/7443/files/Bubble-bg.webp?v=1722250111);
      background-position: center center;
      background-repeat: no-repeat;
      background-size: cover;
  }
  #landing-main-container{
    height:100%;
    flex-direction:column;
    gap:16px;
  }
  #landing-header{
    font-family: var(--family-heavy);
    font-size: 2vw;
    text-align:center;
  }
  .landing-text{
    font-family: var(--family-medium);
    font-size: 16px;
    line-height: 20px;
    text-align:center;
    row-gap: 10px;
    display: flex;
    flex-direction: column;
  }
  .landing-icons{
    display: flex;
    justify-content: center;
    gap: 16px;
    margin-top: 12px;
  }
  .landing-text img{
    width:75px;
    height:75px;
  }
  /* #landing-page-button{
    border-radius: 30px;
    width: 130px;
  } */
  .text-with-image__column--img-container{
    /* mix-blend-mode: multiply; */
  }
  .h-zoom-effect{
    height: 500px
  }
  @media screen and (width <= 768px){
    #landing-main-container{
      padding: 2rem 0;
    }
     #landing-header{
        font-size: 28px;
      }
    .landing-text {
           font-size: 10px;
        line-height: 14px;
    }
    .landing-text img {
          width: 65px;
          height: 64px;
      }
    .h-zoom-effect{
    height: 380px
  }
  }
</style>
