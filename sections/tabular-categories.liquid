{%- style -%}
    .section-{{ section.id }}-margin {
        margin-top: {{ section.settings.margin_top_desktop }}px;
        margin-bottom: {{ section.settings.margin_bottom_desktop }}px;
        margin-inline:auto;
    }
    
    
    .hok-section {
      max-width: 1200px;
      margin: 40px auto;
    }

    .hok-header {
      text-align: center;
    }

    .hok-header h2 {
      font-size: 40px;
      font-family: var(--header-family);
      margin-bottom: 40px;
    }

    .tabs {
      display: flex;
      justify-content: center;
      flex-wrap: wrap;
      gap: 16px;
      margin-bottom: 15px;
    }

    .tab-btn {
      background: #FFF;
      color: #CFCDCD;
      box-shadow: 0px 1px 6px 0px rgba(148, 147, 147, 0.25);
      border: 1px solid #CFCDCD;
      padding: 10px 15px;
      border-radius: 50px;
      cursor: pointer;
      font-size: 20px;
    }

    .tab-btn.active {
      color: #000;
      box-shadow: 0px 1px 8px 0px rgba(63, 62, 58, 0.25);
      font-family:var(--family-heavy);
      border:none;
    }
    .productGrid{
        display: none;    
    }
    .productGrid.active{
        display: flex;
        gap: 20px;   
        overflow-x: auto;
        padding-bottom: 2rem;
    }
    .productGrid.active::-webkit-scrollbar{
        height: 2px;
    }
    .productGrid.active::-webkit-scrollbar-thumb{
        background: #000;
        border-radius: 10px;
    }

    .quote {
      text-align: center;
      font-style: italic;
      font-size: 14px;
      margin-bottom: 25px;
    }

    .products {
      display: flex;
      flex-wrap: wrap;
      gap: 20px;
      justify-content: space-between;
    }

    .tabs-product-card {
      flex: 0 0 calc(33.33% - 20px);
      background: #fff;
      border-radius: 10px;
      overflow: hidden;
      display: flex;
      flex-direction: column;
      transition: 0.3s ease;
    }

    .tabs-product-card img {
      height: auto;
      border-radius: 10px;
    }

    .product-info {
      margin-top: 1rem;
      display: flex;
      gap: 16px;
    }

    .product-info .sequence {
      font-family: Arapey;
      font-size: 56px;
      line-height: normal;
    }
    .product-info h4 {
      margin: 0;
      font-size: 16px;
      font-weight: 600;
    }

    .product-info p {
      font-size: 18px;
      margin: 4px 0;
    }

    .product-info span {
      font-size: 20px;
      color: #000;
      font-family: var(--family-bold);
    }

    @media (max-width: 900px) {
      .tabs-product-card {
        flex: 0 0 calc(45% - 20px);
      }
    }

    @media screen and (max-width: 768px) {
        .section-{{ section.id }}-margin {
            margin-top: {{ section.settings.margin_top_mobile }}px;
            margin-bottom: {{ section.settings.margin_bottom_mobile }}px;
        }
        .hok-header h2{
          font-size: 22px;
          margin-bottom: 16px;
          text-align: start;
        }
        .tabs {
          justify-content: start;
          flex-wrap: nowrap;
          gap: 8px;
          overflow-x: auto;
          padding: 10px 5px;
        }
        .tabs::-webkit-scrollbar {
          display: none;
        }
        .tab-btn {
          padding: 8px 16px;
          font-size: 14px;
          white-space: nowrap;
        }
        .tabs-product-card {
          flex: 0 0 calc(90% - 16px);
      }
      .product-info .sequence {
        font-size: 44px;
      }
      .product-info {
          gap: 8px;
      }
      .product-info span {
        font-size: 14px;
      }
      .product-info p {
          font-size: 12px;
          margin: 0;
      }

    }
  {%- endstyle -%}
  <div class="section-{{ section.id }}-margin">
