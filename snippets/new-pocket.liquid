      assign pdptag = true
      assign img_url = first_variant.featured_image
      assign img_url_1 = product.images[0].src
      assign img_url_2 = product.images[1].src
      for tags in product.tags 
        if tags contains 'New Arrival' 
          assign pdptag = false 
        endif 
