        {% endunless %}
         <div
          style="display: block;"
          class="gw-add-to-wishlist-product-card-placeholder"
          data-gw-product-id="{{ product.id }}"
          data-gw-variant-id="{{ product.first_available_variant.id }}"
        ></div>
      {% if showCross %}
        <div class="handle_product_overlay">
          <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" viewBox="0 0 28 28" fill="none">
            <path d="M10 18L18.0023 10M10 10L18.0023 18" stroke="#212529" stroke-linecap="round"/>
          </svg>
        </div>
      {% endif %}
      <a class="b-product_tile-picture_container customize" href="{{ product.url }}">
        {% if product.images.size > 1 %}
          <img
            class="img-fluid img_url_1"
            src="{{ img_url_1 | image_url }}"
            alt="{{ product.images[0].alt }}"
            width="310"
            height="310"
            fetchpriority="{{ fetch_priority }}"
          >
          <img
            class="img-fluid img_url_2"
            src="{{ img_url_2 | image_url }}"
            alt="{{ product.images[1].alt }}"
            width="310"
            height="310"
            fetchpriority="{{ fetch_priority }}"
          >
        {% else %}
          <img
            class="img-fluid"
            src="{{ product.featured_image | image_url }}"
            alt="{{ product.title }}"
            title="{{ product.title }}"
            width="310"
            fetchpriority="{{ fetch_priority }}"
            height="310"
          >
        {% endif %}
      </a>
    </div>
    {% comment %} PDP Image Ends {% endcomment %}
    {% comment %} PDP Details Starts {% endcomment %}
    <div class="b-product_promo_tile-content">
      {% unless remove_extra_details %} 
       <div class="rating">
            <div
              class="gw-rv-listing-average-placeholder"
              data-product_id="{{ product.id }}"
            ><br></div>
          </div>
      {% endunless %}
      {% unless showCross %}
        {% unless pdptag %}
        <div class="custom_label_badge">
          {% for tags in product.tags %}
            {% if tags contains 'New Arrival' %}
              <div class="custom_badges">NEW IN</div>
            {% endif %}
            {% if tags contains 'label' %}
              {% unless tags contains '_label_New' %}
                {% assign tag_value = tags | split: '_' | slice: 2 %}
                <div
                  class="custom_badges"
                  {% if tag_value == 'Buy Any 2 at 1499' %}
                    style="background:#9d806b;color:#fefefe;"
                  {% endif %}
                >
                  {{ tag_value }}
                </div>
              {% endunless %}
            {% endif %}
          {% endfor %}
        </div>
      {% endunless %}
    {% endunless %}
      <p class="b-product_promo_vendor-name">
        <a class="b-product_promo_vendor-name_link" href="{{- product.url -}}">
          {{- product.vendor | remove: 'BOGO' | remove: '929A' | remove: '929B' | remove: '929B' | remove: 'CLUB' | remove: 'bg-free' | remove: '3-for-2'  -}}
        </a>
        <a class="b-product_promo_tile-name_link" href="{{- product.url -}}">
          {{- product.title  -}}
        </a>
      </p>
      <div class="product-prices d-flex align-items-center {% if template contains 'collection' %}gap-1{%  else %}gap-2{% endif %}">
        <span class="b-product_price-value">
          {{ first_variant.price | money_without_trailing_zeros }}
        </span>
        <span
          class="b-product_price-value rack-rate"
          {% if first_variant.price == first_variant.compare_at_price %}
            style="display:none"
          {% endif %}
        >
          {{ first_variant.compare_at_price | money_without_trailing_zeros }}
        </span>
        {% unless showCross %}
          <span
            class="discount_svg_container position-relative"
            {% if first_variant.inventory_quantity == 0 or first_variant.price == first_variant.compare_at_price %}
              style="display:none"
            {% endif %}
          >
            {% comment %}
              <svg xmlns="http://www.w3.org/2000/svg" width="76" height="20" viewBox="0 0 76 20" fill="none">
                <path d="M0.5 0H75.5C75.5 0 73.3873 5.94689 73.3873 10C73.3873 14.0531 75.5 20 75.5 20H0.5V0Z" class="position-relative" fill="#9D806B"/>
              </svg>
            {% endcomment %}
            <span class="discount_label">
              {{
                first_variant.compare_at_price
                | minus: first_variant.price
                | times: 100.0
                | divided_by: first_variant.compare_at_price
                | money_without_currency
                | times: 100
                | remove: '.0'
              -}}
              % Off
            </span>
          </span>
        {% endunless %}
      </div>
      {% comment %} <div class="variant_container">
        {% if first_variant.option1 != blank and first_variant.option1 != 'Default Title' %}
          Shade: <span class="variant_name">{{ first_variant.option1 }}</span>
        {% endif %}
      </div>
       
      {% if product.available and product.variants.size > 1 %}
      {% assign random_number = 'now' | date: '%s' | modulo: 100 | plus: 1 %}
      {% render 'new-swatches'
      with 'Color' as swatch,
      product: product,
      indexIs: indexIs,
      isrecommend: true,
      isAdvertisement: showCross,
      random_number: random_number,
      handleRecommendSec: recommendedSection
      %}
    {% endif %}
   {% endcomment %}
    {% if product.available and product.variants.size > 1 %}
        <fieldset class="swatch-picker d-flex align-items-center gap-2">
        {% for variant in product.variants limit:3 %}
            <label
                for="swatch-{{ section.id |  remove: '-' |  remove: '_' }}-{{ variant.id }}{{ product.handle | append: value | modulo: 100 | plus: 1 }}"
                class="circular_swatch {% if variant.available %}available{% else %}b-swatch_colors-item m-disabled{% endif %}"
                style="{% if product.variants[forloop.index0].metafields.custom.colour != blank %}background-color:{{ product.variants[forloop.index0].metafields.custom.colour }} !important;{% else %} background-image: url('{% if variant.metafields.custom.variant_image != blank %}{{ variant.metafields.custom.variant_image | image_url }} {% else %}{{ variant.featured_image | image_url }}{% endif %}');{% endif %}"
            >
            </label>
        {% endfor %}
        {% if product.variants.size > 3 %}
            {% liquid
            assign product_count = product.variants.size | minus: 3 %}
            <label class="circular_swatch circular_swatch_additional_count d-center">
               <span>{{ product_count | t |  prepend: '+'  }}</span>
            </label>
        {% endif %}
    </fieldset>
    {% endif %}
      <div class="b-product_promo_tile-buynow mt-auto">
          <div class="b-add_to_bag">
            <input
              type="hidden"
              class="b-add_to_bag-button b-add_to_bagId"
              name="id"
              value="{{ first_variant.id }}"
              min="1"
            >
            <button
              class="b-add_to_bag-button button-cta atc{% unless product.variants.size > 1 %} pocket{% endunless %}"
              type="submit"
              {% if first_variant.inventory_quantity <= 0 %}
                disabled="true"
              {% endif %}
              {% if product.available and product.variants.size > 1 %}
                data-quickviewID="{{ product.id }}"
                data-quickviewHandle="{{ product.handle }}"
                data-quickviewslide="open"
                pdp_MediaData='{{ product.media | json }}'
                {% else %}
                cartid="{{ first_variant.id }}"
              {% endif %}
              >
              {% if first_variant.inventory_quantity > 0 %}
                {% if product.variants.size > 1 %}
                    Select Shade
                    {% else %}
                    Add to Bag
               {% endif %}
              {% else %}Out of Stock{% endif %}
            </button>
          </div>
        
        {% comment %}
           <form
          action="/cart/add"
          method="post"
          id="AddToCartForm{{ first_variant.id }}"
        >
        </form>
           {% if first_variant.inventory_quantity > 0 %}
            <div class="b-add_to_bag">
              <button
                class="b-add_to_bag-button b-add_to_bagId" >
                Out Of Stock
              </button>
            </div>
          {% endif %}
        {% endcomment %}
      </div>
    </div>
    {% comment %} PDP Details Ends {% endcomment %}
  </div>
</div>